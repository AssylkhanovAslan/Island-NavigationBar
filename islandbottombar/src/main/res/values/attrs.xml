<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IslandNavigationBarView">
        <attr name="barMenu" format="reference" />
        <attr name="barSelectedTab" format="integer" />
        <attr name="barTabToggleDuration" format="integer" />
        <attr name="barToggleInterpolator" format="reference" />
        <attr name="barDistribution" format="enum">
            <enum name="spread" value="0" />
            <enum name="packed" value="1" />
            <enum name="spread_inside" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="IslandNavigationTabView">
        <attr name="tabTitle" format="reference|string" />
        <attr name="tabIcon" format="reference" />
        <attr name="tabBackground" format="reference" />
        <attr name="tabToggleDuration" format="integer" />
        <attr name="tabTitleActiveColor" format="reference|color" />
        <attr name="tabTitleInactiveColor" format="reference|color" />
        <attr name="tabCustomPadding" format="dimension|reference" />
    </declare-styleable>
</resources>